    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@react-keycloak/web": "^3.4.0",
    "framer-motion": "^7.6.4",
    "gatsby": "^4.24.7",
    "keycloak-js": "^18.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^1.3.0",
    "type-fest": "^3.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
